{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "foo",
        "bar"
    ],
    "properties": {
        "foo": {
            "type": "string",
            "minLength": 8
        },
        "bar": {
            "type": "integer",
            "maximum": 5
        },
        "baz": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    }
}
